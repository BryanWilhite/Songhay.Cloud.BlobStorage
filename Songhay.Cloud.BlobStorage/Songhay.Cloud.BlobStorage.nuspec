<?xml version="1.0"?>
<package >
  <metadata>
    <id>Songhay.Cloud.BlobStorage</id>
    <version>3.0.0</version>
    <title>Songhay Cloud BLOB Storage</title>
    <authors>bryan.wilhite@gmail.com</authors>
    <owners>rasx@songhaysystem.com</owners>
    <projectUrl>https://github.com/BryanWilhite/Songhay.Cloud.BlobStorage</projectUrl>
    <iconUrl>https://songhaystorage.blob.core.windows.net/studio-public/songhay_icon.png</iconUrl>
    <license type="file">LICENSE.md</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Azure BLOB Storage Repositories for JSON files in the Cloud</description>
    <releaseNotes>added multitargeting support</releaseNotes>
    <copyright>Copyright 2019 Bryan D. Wilhite</copyright>
    <tags>Songhay Azure BLOB Storage JSON</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Configuration" />
      <frameworkAssembly assemblyName="System.Web" />
    </frameworkAssemblies>
    <dependencies>
      <group targetFramework=".NETFramework4.5.2">
        <dependency id="SonghayCore" version="3.2.0" />
        <dependency id="WindowsAzure.Storage" version="9.3.3" />
      </group>
      <group targetFramework=".NETFramework4.6.2">
        <dependency id="SonghayCore" version="3.2.0" />
        <dependency id="WindowsAzure.Storage" version="9.3.3" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="SonghayCore" version="3.2.0" />
        <dependency id="WindowsAzure.Storage" version="9.3.3" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\Songhay.Cloud.BlobStorage*.dll" target="lib\net452" />
    <file src="bin\Release\Songhay.Cloud.BlobStorage*.config" target="lib\net452" />
    <file src="bin\Release\Songhay.Cloud.BlobStorage*.xml" target="lib\net452" />

    <file src="bin\Release\Songhay.Cloud.BlobStorage*.dll" target="lib\net462" />
    <file src="bin\Release\Songhay.Cloud.BlobStorage*.config" target="lib\net462" />
    <file src="bin\Release\Songhay.Cloud.BlobStorage*.xml" target="lib\net462" />

    <file src="bin\Release\netstandard2.0\Songhay.Cloud.BlobStorage*.dll" target="lib\netstandard2.0" />
    <file src="bin\Release\netstandard2.0\Songhay.Cloud.BlobStorage*.json" target="lib\netstandard2.0" />

    <file src="**\*.cs" exclude="**\obj\**" target="src" />

    <file src="..\LICENSE.md" target="" />
  </files>
</package>